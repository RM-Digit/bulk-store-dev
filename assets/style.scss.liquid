span.grid-link__title {
  color: white;
  cursor: pointer;
  display: table;
  /*height: 310px;*/
  left: 0;
  position: absolute;
  top: 0;
  width: 100%/*432px*/;
  font-size: 16px;
  opacity: 0;
  -webkit-transition: opacity 500ms;
  -moz-transition: opacity 500ms;
