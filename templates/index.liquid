<div id="container-cust">
  <!--  Outer wrapper for presentation only, this can be anything you like -->
  <div id="banner-fade" style="max-width: 100%; !important">

    <!-- start Basic Jquery Slider -->
    <ul class="bjqs">



      {% for i in (1..6) %}
      {% capture slide %}slide_{{ i }}{% endcapture %}
      {% capture slide_link %}slide_{{ i }}_link{% endcapture %}
      {% capture slide_img %}slide_{{ i }}.jpg{% endcapture %}
      {% capture slide_alt %}slide_{{ i }}_alt{% endcapture %}

      {% capture slide_header %}slide_{{ i }}_header{% endcapture %}
      {% capture slide_body %}slide_{{ i }}_body{% endcapture %}

      {% capture button_1_enabled %}slide_{{ i }}_button_1_enabled{% endcapture %}
      {% capture button_1_text %}slide_{{ i }}_button_1_text{% endcapture %}
      {% capture button_1_link %}slide_{{ i }}_button_1_url{% endcapture %}

      {% capture button_2_enabled %}slide_{{ i }}_button_2_enabled{% endcapture %}
      {% capture button_2_text %}slide_{{ i }}_button_2_text{% endcapture %}
      {% capture button_2_link %}slide_{{ i }}_button_2_url{% endcapture %}

      {% capture mobile_slide_img %}mobile_slide_{{ i }}.jpg{% endcapture %}

      {% assign mobile_slide_img_url = mobile_slide_img | asset_url %}

      {% capture slide_link %}slide_{{ i }}_link{% endcapture %}



      {% if settings[slide] %}
      <li>

        <div class="slide {% if settings[slide_link].size > 1 %}clickable{% endif %}" id="slide_{{ i }}" data-mobile="{{ mobile_slide_img_url }}"
             style="background-image:url('{{ slide_img | asset_img_url: '1920x' }}'); height:100%;"
             {% if settings[slide_link].size > 1 %}
             onclick="window.location='https://{{ settings[slide_link] | replace: "http://", "" | replace: "https://", "" }}'"
             {% endif %}
             >

          <div class="grid--full">
            <div class="grid__item small--one-whole medium--one-whole large--one-half" style="text-align: center;">
              <div class="hero-text-container" style="">
                <div class="hero-text-header asphaltic" style="margin-bottom:20px; color:white; ">{{ settings[slide_header]}}</div>
                <div class="hero-text-body" style="text-align: left; margin-bottom:20px;">{{ settings[slide_body]}}</div>

                {% if settings[button_1_enabled] %}
                <div style="display: inline-block; margin: 0 0px 0px 0;">
                  <div class="hero-button-container"
                       style="padding: 15px; background-image:url('{{ 'redWashShopNow.png' | asset_url  }}');  background-repeat: no-repeat;
                              background-position: center; background-size: contain; position: relative;">
                    <a href="{{ settings[button_1_link] }}" class="hero-button"
                       style="display:inline-block; border:1px solid white; padding: 8px 12px;">
                      <div class="asphaltic" style="color: white; font-size: 2.8em;">
                        {{ settings[button_1_text] }}
                      </div>
                    </a>
                  </div>
                </div>
                {% endif %}

                {% if settings[button_2_enabled] %}
                <div style="display: inline-block; margin: 0 0px 10px 0;">
                  <div class="hero-button-container"
                       style="padding: 15px;background-image:url('{{ 'redWash.png' | asset_url  }}');  background-repeat: no-repeat;
                              background-position: center; background-size: contain; position: relative;">
                    <a href="{{ settings[button_2_link] }}" class="hero-button"
                       style="display:inline-block; border:1px solid white; padding: 8px 12px;">
                      <div class="asphaltic" style="color: white; font-size: 2.8em;">
                        {{ settings[button_2_text] }}
                      </div>
                    </a>
                  </div>
                </div>
                {% endif %}


              </div>
            </div>

            <div class="grid__item small--zero medium--zero large--one-half"></div>
          </div>
          <!--<img src="{{ 'hero_sales_ribbon.png' | asset_url }}" style="position: absolute; right:0; bottom:0;">-->

          {% if settings[slide_link].size > 1 %}
          </a>
        {% endif %}

      </div>



    </li>
  {% endif %}
  {% endfor %}




  </ul>
<!-- end Basic jQuery Slider -->

</div>
<!-- End outer wrapper -->

